<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.climbingguide"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.climbingguide.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name="org.cimbingguide.gui.ShowAreas" android:label="@string/app_name"/>
         <activity android:name="org.cimbingguide.gui.ShowAll" android:label="@string/app_name"/>
          <activity android:name="org.cimbingguide.gui.ShowSectors" android:label="@string/app_name"/>
        <activity android:name="org.cimbingguide.gui.CreateRoute" android:label="@string/app_name"/>
          
         <activity android:name="com.example.climbingguide.MarekLauncher" android:label="@string/app_name"/>
          <activity android:name="com.example.climbingguide.LukasLauncher" android:label="@string/app_name"/>
          
    </application>

</manifest>
